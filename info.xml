<?xml version="1.0"?>
<extension key="org.civicrm.sms.alinto" type="module">
  <file>alinto</file>
  <name>alinto</name>
  <description>This extension provides SMS integration with Alinto.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Renaud Fradin and Anatole Allain</author>
    <email>dev@all-in-appli.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://civicrm.org</url>
    <url desc="Documentation">https://github.com/allinappli/alinto</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2020-12-30</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.45</ver>
  </compatibility>
  <comments>For support, please contact project team on dev@all-in-appli.com.</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/Alinto</namespace>
    <format>24.09.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>smarty-v2@1.0.3</mixin>
    <mixin>entity-types-php@2.0.0</mixin>
  </mixins>
</extension>
